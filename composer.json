{
    "name": "league/torrent",
    "description": "League/torrent is a lightweight PHP library for work with torrents",
    "keywords": [
        "league",
        "league/torrent"
    ],
    "homepage": "https://github.com/thephpleague/torrent",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilya Gusev",
            "email": "mail@igusev.ru",
            "homepage": "https://php-cat.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "League\\Torrent\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Torrent\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
